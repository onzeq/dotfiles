syntax on
set tabstop=4
set shiftwidth=4
set softtabstop=4
set autoindent
set smartindent
let mapleader = "\<Space>"
let g:mapleader = "\<Space>"
nnoremap <Leader>cc :set colorcolumn=80<cr>
nnoremap <Leader>ncc :set colorcolumn=-80<cr>
inoremap jj <esc>
nnoremap <Leader>o o<Esc>
nnoremap <Leader>O O<Esc>
set number
set rnu
set splitright
set clipboard=unnamedplus
